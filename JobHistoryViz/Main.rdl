<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="Quack">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=ANDROMEDA\QUACK</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>72600302-6303-47de-aef7-90f6e7382563</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="JobHistory">
      <Query>
        <DataSourceName>Quack</DataSourceName>
        <CommandText>select
	j.name,
	jh.run_duration,
	RunDateTime=msdb.dbo.agent_datetime(jh.run_date, jh.run_time)
from 
	msdb.dbo.sysjobhistory as jh
	inner join 
	msdb.dbo.sysjobs as j
		on jh.job_id=j.job_id
where 
	step_name='(Job outcome)' and
	run_duration&gt;0
order by
	RunDateTime desc</CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="run_duration">
          <DataField>run_duration</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RunDateTime">
          <DataField>RunDateTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
